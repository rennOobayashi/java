package week2;

public class Print {
	//필수 메소드(JVM의 시작점)
	//자동 완성 - ctrl + space bar
	public static void main(String[] args) {
		//실행 - f11 or ctrl + f11
		//sysout + strl + space bar
		System.out.println("객체지향 프로그래밍");
	
		//주석 단축 - ctrl + /, 
		/*System.out.println("객체");
		System.out.println("지향");*/
		System.out.println("프로그래밍");
	}
}
