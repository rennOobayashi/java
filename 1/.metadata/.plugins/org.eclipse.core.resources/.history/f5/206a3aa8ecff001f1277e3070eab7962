package week2;

public class DataType3 {
	public static void main(String[] args) {
		//char 데이터 타입(유니코드)
		//char 타입은 음수가 없음
		
		char cData1 = 'A';
		char cData2 = 65; //ASCII
		char cData3 = '\u0041'; //UNICODE
		
		System.out.println("cData1 = " + cData1);
		System.out.println("cData2 = " + cData2);
		System.out.println("cData3 = " + cData3);
		
	}
}
