package week2;

public class DataType1 {
	public static void main(String[] args) {
		//기본 데이터 타입
		//직접 값을 주는 초기값: 리터럴
		//정수
		byte bData  = 65; //1byte
		char cData  = 65; //2byte
		short sData = 65; //2byte
		int iData   = 65; //4byte
		long lData  = 65; //8byte
		
		//실수
		float fData = 65.12f; //4byte
		float double = 65.12f; //8byte 
	}
}
